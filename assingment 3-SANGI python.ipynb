    "for i in range(1042000,70264865):\n",
    "    num=i\n",
    "    sum=0\n",
    "    temp=num\n",
    "    n=len(str(num))\n",
    "    while temp>0:\n",
    "        digit=temp%10\n",
    "        sum+=digit**n\n",
    "        temp//=10\n",
    "    if num==sum:\n",
    "        print(num,\"= is a first amstrong number\")\n",
    "        break"
